<form action="/edit-kuisioner" class="form-post" method="post">
  <h5>Edit Kuisioner</h5>
  <div class="mb-3">
    <input type="text" class="title" name="title" placeholder="Enter title here" value="<%= datakuisioner.title %>">
  </div>
  <div class="mb-3">
    <input type="text" class="excerpt" name="excerpt" placeholder="Short description (25 words)" value="<%= datakuisioner.excerpt %>">
  </div>
  <input id="body" type="hidden" name="description" value="<%= datakuisioner.description %>">
  <trix-editor input="body"></trix-editor>
  <div class="mt-3 con-btn">
    <a href="/account" class="btn-cancel">Cancel</a>
    <button type="submit" class="btn-publish">Publish</button>
  </div>
</form>





